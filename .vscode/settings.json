{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.idea": true,
        "**/.vs": true,
        "**/.vscode": true,
        "SportsStore/bin": true,
        "**/bin": true,
        "SportsStore.csproj": true,
        "SportsStore/SportsStore.csproj": true,
        ".gitignore": true,
        "SportsStore.sln": true,
        "**/SportsStore.sln": true,
        "SportsStore/obj": true,
        "**/obj": true,
        "**/SportsStore.Tests.csproj": true,
        "**/SportsStore.sln.DotSettings.user": true
    },
    "explorerExclude.backup": null,
    "cmake.configureOnOpen": false,
    "sqltools.connections": [
        {
            "mssqlOptions": {
                "appName": "SQLTools",
                "useUTC": true,
                "encrypt": true
            },
            "previewLimit": 50,
            "server": "localhost",
            "port": 1433,
            "askForPassword": true,
            "driver": "MSSQL",
            "connectString": "Server=localhost\\SQLEXPRESS;Database=SportsStore;Trusted_Connection=True;",
            "name": "SportsStore"
        }
    ]
}