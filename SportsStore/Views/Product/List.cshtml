@model ProductListViewModel

@foreach (var product in Model.Products) {
    <div>
        <h3>@product.Name</h3>
        @product.Description
        <h4>@product.Price.ToString("C")</h4>
    </div>
}
@if (Model.PagingInfo != null) {
    <div page-model="@Model.PagingInfo" page-action="List"></div>
}